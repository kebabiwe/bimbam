<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">
    <RelativeLayout
            android:id="@+id/edit_task"
            android:layout_width="320dp"
            android:layout_height="352dp"
            android:clipToOutline="true"
            android:background="@drawable/edit_task"
            tools:ignore="MissingConstraints">
        <RelativeLayout
                android:id="@+id/form"
                android:layout_width="288dp"
                android:layout_height="56dp"
                android:layout_centerHorizontal="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="40dp"
                android:clipToOutline="true"
                android:background="@drawable/form"
                android:elevation="4dp">
            <EditText
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="16dp"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="text"
                    android:hint="Название"
                    android:fontFamily="@font/opensans"
                    android:ems="10"
                    android:textColor="@color/black"
                    android:background="@android:color/transparent"
                    android:text="Название"
                    android:id="@+id/editTextText"/>
        </RelativeLayout>
        <RelativeLayout
                android:id="@+id/date"
                android:layout_width="136dp"
                android:layout_height="46dp"
                android:clipToOutline="true"
                android:background="@drawable/form"
                android:elevation="4dp"
                android:layout_alignParentBottom="true" android:layout_marginBottom="193dp"
                android:layout_alignParentStart="true" android:layout_marginStart="21dp">
            <View
                    android:id="@+id/vector1"
                    android:layout_width="20dp"
                    android:layout_height="9dp"
                    android:background="@drawable/vector_1"
                    android:alpha="0.5"
                    android:layout_marginLeft="50dp" android:layout_marginRight="-50dp"
                    android:layout_alignParentEnd="true" android:layout_marginEnd="18dp"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="18dp"/>
            <TextView
                    android:id="@+id/some_id"
                    android:layout_width="36dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="16dp"
                    android:layout_centerVertical="true"
                    android:text="Дата"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:alpha="0.5"
                    android:fontFamily="@font/opensans"
            />
        </RelativeLayout>
        <RelativeLayout
                android:id="@+id/time"
                android:layout_width="133dp"
                android:layout_height="45dp"
                android:clipToOutline="true"
                android:background="@drawable/form"
                android:elevation="4dp"
                tools:layout_editor_absoluteY="402dp" tools:layout_editor_absoluteX="115dp"
                android:layout_alignParentEnd="true" android:layout_marginEnd="13dp"
                android:layout_alignParentBottom="true" android:layout_marginBottom="193dp">
            <View
                    android:id="@+id/vector"
                    android:layout_width="20dp"
                    android:layout_height="9dp"
                    android:background="@drawable/vector_1"
                    android:alpha="0.5"
                    android:layout_marginLeft="50dp" android:layout_marginRight="-50dp"
                    android:layout_alignParentEnd="true" android:layout_marginEnd="18dp"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="18dp"/>
            <TextView
                    android:id="@+id/some_id1"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="16dp"
                    android:layout_centerVertical="true"
                    android:text="Время"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:alpha="0.5"
                    android:fontFamily="@font/opensans"
            />
        </RelativeLayout>
        <RelativeLayout
                android:id="@+id/form11"
                android:layout_width="288dp"
                android:layout_height="56dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="172dp"
                android:clipToOutline="true"
                android:background="@drawable/form"
                android:elevation="4dp"
                android:layout_alignParentEnd="true" android:layout_marginEnd="12dp">
            <EditText
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="16dp"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="text"
                    android:hint="Название"
                    android:fontFamily="@font/opensans"
                    android:ems="10"
                    android:textColor="@color/black"
                    android:background="@android:color/transparent"
                    android:text="Описание"
                    android:id="@+id/editTextText2"/>
        </RelativeLayout>
        <RelativeLayout
                android:id="@+id/save"
                android:layout_width="105dp"
                android:layout_height="41dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="273dp"
                android:clipToOutline="true"
                android:background="@drawable/frame_27"
                android:layout_alignParentEnd="true" android:layout_marginEnd="25dp">
            <TextView
                    android:id="@+id/save1"
                    android:layout_width="81dp"
                    android:layout_height="wrap_content"
                    android:text="Сохранить"
                    android:textAppearance="@style/some_id"
                    android:textColor="@color/white1"
                    android:fontFamily="@font/opensans"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="center"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="9dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="12dp" android:layout_marginTop="10dp"
                    android:layout_alignParentTop="true" android:layout_alignParentStart="true"
                    android:layout_marginStart="12dp"/>

        </RelativeLayout>
        <RelativeLayout
                android:id="@+id/no"
                android:layout_width="105dp"
                android:layout_height="41dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="273dp"
                android:clipToOutline="true"
                android:background="@drawable/frame_26"
                android:layout_alignParentEnd="true" android:layout_marginEnd="181dp">
            <TextView
                    android:id="@+id/delete1"
                    android:layout_width="81dp"
                    android:layout_height="wrap_content"
                    android:text="Удалить"
                    android:gravity="center"
                    android:textAppearance="@style/some_id"
                    android:textColor="@color/white1"
                    android:fontFamily="@font/opensans"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"

                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="10dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="12dp" android:layout_marginTop="9dp"
                    android:layout_alignParentTop="true" android:layout_alignParentStart="true"
                    android:layout_marginStart="12dp"/>

        </RelativeLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>