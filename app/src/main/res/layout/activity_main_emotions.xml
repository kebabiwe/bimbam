<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity_emotions">
    <View
            android:id="@+id/rectangle_5"
            android:layout_width="0dp"
            android:layout_height="194dp"
            android:layout_marginTop="-30dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/rectangle_5"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"/>


    <View
            android:layout_marginLeft="16dp"
            android:layout_marginTop="133dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            app:layout_constraintHorizontal_bias="0.041"
            app:layout_constraintVertical_bias="0.0"
            android:id="@+id/arrow"
            android:layout_alignParentBottom="true"
            android:background="@drawable/arrow"
            tools:ignore="MissingConstraints"/>/>
    <TextView
            android:layout_marginLeft="16dp"
            android:layout_marginTop="133dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            app:layout_constraintHorizontal_bias="0.16"
            app:layout_constraintVertical_bias="0.0"
            android:fontFamily="@font/opensans"
            android:textColor="#FFFFFF"
            android:id="@+id/some_id"
            android:layout_width="81dp"
            android:layout_height="22dp"
            android:text="Вернуться"
            android:textAppearance="@style/some_id"
            android:lineSpacingExtra="1sp"
            android:translationY="-0.31sp"
            android:gravity="top"
            android:textSize="16dp"
            tools:ignore="MissingConstraints"/>
    <RelativeLayout
            android:layout_marginTop="10dp"
            android:id="@+id/navbar"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="false"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.023">

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="40dp"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/logo"
                android:id="@+id/imageView2"
                tools:ignore="MissingConstraints"/>

        <View
                android:id="@+id/profile"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="40dp"
                android:layout_centerVertical="true"
                android:background="@drawable/profile"
                tools:ignore="MissingConstraints"/>
    </RelativeLayout>

    <RelativeLayout
            android:id="@+id/tabbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="#FFFFFF"
            tools:ignore="MissingConstraints"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginLeft="4dp"
            android:layout_marginBottom="0dp">

        <View
                android:id="@+id/icon1"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:background="@drawable/icon__2"
                android:alpha="1"
        />

        <View
                android:id="@+id/icon2"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon1"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="0.5"/>

        <View
                android:id="@+id/icon3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon2"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon_3"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="0.5"/>

        <View
                android:id="@+id/icon4"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon3"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon_4"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="0.5"/>

        <View
                android:id="@+id/icon5"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon4"
                android:layout_marginStart="35dp"
                android:layout_marginEnd="8dp"
                android:background="@drawable/icon_5"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="1.0"/>

    </RelativeLayout>

    <TextView
            android:layout_marginLeft="0dp"
            android:scaleType="center"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:fontFamily="@font/pattaya"
            android:layout_centerInParent="true"
            android:textColor="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"
            android:textSize="32sp"
            tools:ignore="MissingConstraints"
            android:id="@+id/title"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="80dp"
            android:text="Эмоции и чувства"
            android:textAppearance="@style/title"
            android:lineSpacingExtra="-7sp"
            android:gravity="center_horizontal|top"
    />
    <RelativeLayout
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.815"
            android:layout_centerHorizontal="true"
            tools:ignore="MissingConstraints"
            android:textColor="#FFFFFF"
            android:gravity="center_horizontal|top"
            android:layout_marginTop="10dp"
            android:id="@+id/frame_31"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:clipToOutline="true"
            android:background="#FFFFFF"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintBottom_toTopOf="@+id/tabbar"
            app:layout_constraintTop_toBottomOf="@+id/rectangle_5"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">


        <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/image"
                tools:ignore="NotSibling">

            <LinearLayout
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    app:layout_constraintHorizontal_bias="0.16"
                    app:layout_constraintVertical_bias="0.0"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <RelativeLayout
                        android:background="@drawable/frame"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_alignParentRight="true"
                        app:layout_constraintHorizontal_bias="0.16"
                        app:layout_constraintVertical_bias="0.0"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginLeft="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"
                        android:clipToOutline="true"
                        android:layout_marginBottom="16dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:gravity="center"
                        android:elevation="4dp"
                        android:layout_marginEnd="16dp"
                        android:id="@+id/frame_32"
                        tools:ignore="DuplicateIds">

                    <View
                            android:layout_marginLeft="0dp"
                            android:layout_marginRight="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginBottom="0dp"
                            android:id="@+id/rectangle_8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:background="@drawable/restanges_9"
                            tools:ignore="DuplicateIds"/>
                    <TextView
                            android:layout_marginLeft="16dp"
                            android:layout_marginRight="16dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginBottom="16dp"
                            android:fontFamily="@font/opensans"
                            android:textColor="#115931"
                            android:textSize="20dp"
                            android:translationY="-0.27sp"
                            android:id="@+id/some_id34"
                            android:layout_width="291dp"
                            android:layout_height="wrap_content"
                            android:text="Активный и самостоятельный"
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="-1sp"
                            android:gravity="top"
                            android:layout_below="@+id/rectangle_8"/>
                    <TextView
                            android:layout_marginTop="16dp"
                            android:layout_marginBottom="16dp"
                            android:fontFamily="@font/opensans"
                            android:textColor="@color/black"
                            android:id="@+id/some_id35"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_marginLeft="16dp"
                            android:layout_marginRight="16dp"
                            android:textSize="16dp"
                            android:text="Родитель обязан помочь ребёнку, но именно помочь, а не сделать за него. Самое сложное — наблюдать неудачные попытки ребёнка, его усталость, порой отчаяние. Выдержать это напряжение, это состояние сознательной беспомощности — ваша задача и великий подвиг родительской любви. Надо осознать, что ваша вера в возможности и силы ребёнка, придают ему мужество."
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="1sp"
                            android:translationY="-0.31sp"
                            android:gravity="top"
                            android:layout_below="@+id/some_id34"/>

                </RelativeLayout>



            </LinearLayout>
        </ScrollView>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>