            android:layout_width="408dp"
            android:layout_height="63dp"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="668dp"
            app:layout_constraintHorizontal_bias="0.333">

        <View
                android:id="@+id/icon1"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_marginStart="27dp"
                android:background="@drawable/icon1"
                android:alpha="1"
        />

        <View
                android:id="@+id/icon2"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon1"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon2"
                android:alpha="1"
        />

        <View
                android:id="@+id/icon3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon2"
                android:layout_marginStart="37dp"
                android:background="@drawable/divider1"
                android:alpha="1"
        />

        <View
                android:id="@+id/icon4"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon3"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon3"
                android:alpha="1"
        />

        <View
                android:id="@+id/icon5"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon4"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon4"
                android:alpha="1"
        />

    </RelativeLayout>
    <RelativeLayout
            android:layout_centerHorizontal="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:id="@+id/frame_30.2"
            android:layout_width="366dp"
            android:layout_height="55dp"
            android:clipToOutline="true"

            android:elevation="8dp"
            tools:layout_editor_absoluteY="318dp" tools:layout_editor_absoluteX="21dp"
            tools:ignore="MissingConstraints">
        <TextView
