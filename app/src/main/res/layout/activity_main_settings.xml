                android:textAppearance="@style/some_id"
                android:lineSpacingExtra="1sp"
                android:translationY="-0.31sp"
                android:gravity="top"
        />
        <Switch
                android:id="@+id/switch1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/frame281"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="280dp"
                android:layout_centerVertical="true"
                android:clipToOutline="true"
        />


    </RelativeLayout>
    <RelativeLayout
            android:layout_centerHorizontal="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"


            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:id="@+id/frame_30.3"
            android:layout_width="366dp"
            android:layout_height="55dp"
            android:clipToOutline="true"
            android:elevation="8dp"
            tools:ignore="MissingConstraints" app:layout_constraintHorizontal_bias="0.555"
            app:layout_constraintVertical_bias="0.615">
        <TextView
                android:id="@+id/some_id2"
                android:layout_width="78dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="16dp"
                android:layout_centerVertical="true"
                android:text="Вибрация"
                android:textAppearance="@style/some_id"
                android:lineSpacingExtra="1sp"
                android:translationY="-0.31sp"
                android:gravity="top"
        />
        <Switch
                android:id="@+id/switch1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/frame281"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="280dp"
                android:layout_centerVertical="true"
                android:clipToOutline="true"
        />
    </RelativeLayout>
    <RelativeLayout


            android:layout_centerHorizontal="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:id="@+id/frame_30.1"
            android:layout_width="366dp"
            android:layout_height="55dp"
            android:clipToOutline="true"
            android:elevation="8dp"
            tools:ignore="MissingConstraints" app:layout_constraintHorizontal_bias="0.511"
            app:layout_constraintVertical_bias="0.729">
        <TextView
                android:id="@+id/some_id1"
                android:layout_width="146dp"
                android:layout_height="21dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="16dp"
                android:layout_centerVertical="true"
                android:text="Выйти из аккаунта"
                android:textAppearance="@style/some_id1"
                android:lineSpacingExtra="-1sp"
                android:gravity="top"
        />
    </RelativeLayout>
