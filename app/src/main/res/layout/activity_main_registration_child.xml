<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
tools:context=".MainActivity_registrationChild">
    <RelativeLayout
            android:id="@+id/sign_up"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:background="#F9F9F9"
            tools:ignore="MissingConstraints" tools:layout_editor_absoluteY="79dp"
            tools:layout_editor_absoluteX="182dp">
        <!-- Auto layout, variables, and unit scale are not yet supported -->

        <RelativeLayout
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="410dp"
                android:layout_centerHorizontal="true"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintVertical_bias="0.376"
                android:id="@+id/form5"
                android:layout_alignParentTop="true"
                android:background="@drawable/form"
                tools:ignore="MissingConstraints">
            <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:textColor="@color/black"
                    android:id="@+id/childbirthday"
                    android:background="@android:color/transparent" android:layout_alignParentBottom="true"
                    android:layout_marginBottom="16dp" android:layout_alignParentEnd="true"
                    android:layout_marginEnd="85dp" android:layout_alignParentStart="true"
                    android:layout_marginStart="25dp" android:layout_marginTop="16dp"
                    android:layout_alignParentTop="true"/>
        </RelativeLayout>
        <TextView
                android:textSize="16dp"
                android:id="@+id/text3"
                android:layout_width="119dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="Дата рождения"
                android:fontFamily="@font/opensans"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:layout_marginLeft="36dp" android:layout_marginTop="383dp"
        />


        <TextView
                android:id="@+id/text2"
                android:layout_width="102dp"
                android:layout_height="22dp"
                android:textSize="16dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="Имя ребенка"
                android:fontFamily="@font/opensans"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:layout_marginLeft="38dp" android:layout_marginTop="189dp"
        />
        <TextView
                android:textSize="16dp"
                android:layout_width="31dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:id="@+id/text1"
                android:text="Пол"
                android:fontFamily="@font/opensans"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:layout_marginLeft="48dp" android:layout_marginTop="287dp"
        />
        <RelativeLayout

                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="216dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_alignParentTop="true"
                android:clipToOutline="true"
                android:layout_centerHorizontal="true"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintVertical_bias="0.376"
                android:id="@+id/form1"
                android:background="@drawable/form"
                tools:ignore="MissingConstraints"
        >
            <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_centerVertical="true"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:textColor="@color/black"
                    android:id="@+id/childname"
                    android:background="@android:color/transparent" android:layout_alignParentStart="true"
                    android:layout_marginStart="20dp" android:layout_alignParentEnd="true"
                    android:layout_marginEnd="90dp"/>
        </RelativeLayout>

        <TextView
                android:layout_marginLeft="60dp"
                android:layout_width="wrap_content"
                android:layout_height="67dp"
                android:layout_alignParentTop="true"
                android:text="Введите данные"
                android:textAppearance="@style/title"
                android:lineSpacingExtra="0sp"
                android:gravity="center_vertical|center_horizontal"
                android:fontFamily="@font/pattaya"
                android:layout_marginTop="112dp"
                app:layout_constraintTop_toTopOf="parent"/>
        <TextView
                android:textSize="16dp"
                android:id="@+id/text5"
                android:layout_width="238dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="Индивидуальная особенность"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:fontFamily="@font/opensans"
                android:layout_marginLeft="48dp" android:layout_marginTop="482dp"
        />
        <RelativeLayout
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_centerHorizontal="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="607dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintVertical_bias="0.376"
                android:id="@+id/button"
                android:layout_marginLeft="45dp"
                android:clipToOutline="true"
                android:background="@drawable/button0">
        </RelativeLayout>
        <Spinner
                android:layout_centerHorizontal="true"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintVertical_bias="0.376"
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_alignParentTop="true"
                android:clipToOutline="true"
                tools:ignore="MissingConstraints"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="313dp"
                android:id="@+id/spinner"
                style="@style/SpinnerStyle"
                android:layout_marginEnd="47dp"
                android:layout_marginBottom="-133dp"
                android:layout_marginStart="37dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentStart="true"
                android:background="@drawable/form"
                android:spinnerMode="dropdown"
                android:popupBackground="@drawable/sex"/>
        <Spinner
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_marginTop="506dp"
                android:layout_marginLeft="35dp"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="47dp"
                android:layout_marginStart="37dp"
                android:id="@+id/spinner1"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:background="@drawable/form"
                android:gravity="center_vertical"
                android:popupBackground="@drawable/diagnos"
                android:spinnerMode="dropdown"
                android:textAlignment="gravity"
                android:layout_below="@+id/form5"
                android:layout_marginBottom="105dp"
                android:layout_alignBottom="@+id/button"/>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>