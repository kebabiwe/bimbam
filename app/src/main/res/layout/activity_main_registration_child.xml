<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
tools:context=".MainActivity_registrationChild">
    <RelativeLayout
            android:id="@+id/sign_up"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:background="#F9F9F9"
            tools:ignore="MissingConstraints">
        <!-- Auto layout, variables, and unit scale are not yet supported -->

        <RelativeLayout
                android:layout_centerHorizontal="true"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintVertical_bias="0.376"
                android:id="@+id/form5"
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_alignParentTop="true"

                android:background="@drawable/form"
                tools:ignore="MissingConstraints"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="346dp"/>
        <TextView
                android:id="@+id/text3"
                android:layout_width="119dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="Дата рождения"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:layout_marginLeft="40dp" android:layout_marginTop="319dp"
        />
        <TextView
                android:id="@+id/text2"
                android:layout_width="102dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="Имя ребенка"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:layout_marginLeft="40dp" android:layout_marginTop="144dp"
        />

        <RelativeLayout
                android:layout_centerHorizontal="true"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494" app:layout_constraintVertical_bias="0.376"
                android:id="@+id/form2"
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_alignParentTop="true"
                android:clipToOutline="true"
                android:background="@drawable/form"
                tools:ignore="MissingConstraints"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="259dp">

            <Spinner
                    android:layout_marginLeft="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="55dp" android:id="@+id/spinner"
                    style="@style/CustomSpinnerDropDownItem"
                    android:layout_alignParentEnd="true" android:layout_marginEnd="0dp"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="0dp"
                    android:layout_alignParentStart="true" android:layout_marginStart="0dp"
                    android:layout_marginTop="1dp" android:layout_alignParentTop="true"/>
        </RelativeLayout>
        <TextView
                android:layout_width="31dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:id="@+id/text1"
                android:text="Пол"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:layout_marginLeft="40dp" android:layout_marginTop="232dp"
        />
        <RelativeLayout
                android:layout_centerHorizontal="true"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintVertical_bias="0.376"
                android:id="@+id/form1"
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_alignParentTop="true"
                android:clipToOutline="true"
                android:background="@drawable/form"
                android:layout_marginLeft="35dp"
                tools:ignore="MissingConstraints"
                android:layout_marginTop="172dp">
            <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_centerVertical="true"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:textColor="@color/black"
                    android:id="@+id/childname"
                    android:background="@android:color/transparent"/>
        </RelativeLayout>

        <TextView
                android:id="@+id/title"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_alignParentTop="true"
                android:gravity="center"
                android:text="Введите данные"
                android:fontFamily="@font/pattaya"
                android:textAppearance="@style/title"
                android:lineSpacingExtra="0sp"
                android:layout_marginTop="71dp"/>

        <RelativeLayout
                android:layout_centerHorizontal="true"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintVertical_bias="0.376"
                android:id="@+id/form3"
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_marginTop="437dp"
                android:layout_marginLeft="35dp"
                android:layout_alignParentTop="true"
                android:clipToOutline="true"
                android:background="@drawable/form">
        </RelativeLayout>
        <TextView
                android:id="@+id/text5"
                android:layout_width="238dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="Индивидуальная особенность"
                android:textAppearance="@style/text"
                android:gravity="top"
                android:layout_marginLeft="40dp" android:layout_marginTop="410dp"
        />

        <RelativeLayout
                android:layout_centerHorizontal="true"
                tools:ignore="MissingConstraints"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.494" app:layout_constraintVertical_bias="0.376"
                android:id="@+id/button"
                android:layout_width="320dp"
                android:layout_height="56dp"
                android:layout_marginLeft="45dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="569dp"
                android:clipToOutline="true"
                android:background="@drawable/button0"

        >
        </RelativeLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>