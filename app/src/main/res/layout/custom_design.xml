<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/frame__11"
        >

    <RelativeLayout
            android:gravity="center_horizontal|top"
            android:layout_width="340dp"
            android:layout_height="203dp"
            android:layout_gravity=""
            android:clipToOutline="true"
            android:id="@+id/log_out"
            android:background="@drawable/frame__11"
            tools:ignore="MissingConstraints">
        <TextView
                android:layout_centerHorizontal="true"
                android:clipToOutline="true"
                app:layout_constraintHorizontal_bias="0.16"
                app:layout_constraintVertical_bias="0.0"
                android:gravity="center"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="35dp"
                tools:ignore="MissingConstraints"
                android:id="@+id/some_id"
                android:text="Выход"
                android:fontFamily="@font/pattaya"
                android:textAppearance="@style/logout"
                android:lineSpacingExtra="-7sp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="110dp"
                android:layout_marginTop="25dp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="134dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="90dp"/>
        <TextView
                android:layout_centerHorizontal="true"
                android:clipToOutline="true"
                app:layout_constraintHorizontal_bias="0.16"
                app:layout_constraintVertical_bias="0.0"
                android:gravity="center"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_alignParentRight="true"
                android:layout_marginStart="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="35dp"
                tools:ignore="MissingConstraints"
                android:id="@+id/text"
                android:layout_width="247dp"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="30dp"
                android:text="Вы действительно хотите выйти?"
                android:textAppearance="@style/some_id"
                android:fontFamily="@font/opensans"
                android:textColor="@color/black"
                android:lineSpacingExtra="1sp"
                android:translationY="-0.27sp"

                android:layout_marginBottom="93dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="60dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="88dp"/>

        <RelativeLayout
                android:id="@+id/frame_25"
                android:layout_width="125dp"
                android:layout_height="56dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="136dp"
                android:clipToOutline="true"
                android:elevation="4dp"
                android:background="@drawable/list_presssed"

        >
            <TextView
                    android:id="@+id/no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Отмена"
                    android:textColor="@color/black"
                    android:fontFamily="@font/opensans"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="center_horizontal|top"
                    android:textSize="22sp"
                    android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout
                android:id="@+id/frame_26"
                android:layout_width="126dp"
                android:layout_height="56dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="171dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="136dp"
                android:clipToOutline="true"
                android:background="@drawable/frame__26"
                android:elevation="4dp"
        >
            <TextView
                    android:id="@+id/yes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Выход"
                    android:fontFamily="@font/opensans"
                    android:textSize="22sp"
                    android:textColor="@color/white1"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="center_horizontal|top"
                    android:layout_centerInParent="true"/>
        </RelativeLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>