<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">
    <RelativeLayout
            android:id="@+id/log_out"
            android:layout_width="320dp"
            android:layout_height="203dp"
            android:clipToOutline="true"
            android:background="@drawable/log_out"
            tools:ignore="MissingConstraints">
        <TextView
                android:id="@+id/some_id"
                android:layout_width="99dp"
                android:layout_height="44dp"
                android:layout_alignParentTop="true"
                android:text="Выход"
                android:fontFamily="@font/pattaya"
                android:textAppearance="@style/logout"
                android:lineSpacingExtra="-7sp"
                android:gravity="center_horizontal|top"
                tools:ignore="MissingConstraints"
                android:layout_alignParentEnd="true" android:layout_marginEnd="118dp" android:layout_marginTop="25dp"
                android:layout_alignParentBottom="true" android:layout_marginBottom="134dp"
                android:layout_alignParentStart="true" android:layout_marginStart="103dp"/>
        <TextView
                android:id="@+id/text"
                android:layout_width="229dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="39dp"
                android:text="Вы действительно хотите выйти?"
                android:textAppearance="@style/some_id"
                android:fontFamily="@font/opensans"
                android:textColor="@color/black"
                android:lineSpacingExtra="1sp"
                android:translationY="-0.27sp"
                android:gravity="center_horizontal|top"
                android:layout_marginBottom="93dp" android:layout_alignParentEnd="true"
                android:layout_marginEnd="52dp" android:layout_alignParentBottom="true"/>
        <RelativeLayout
                android:id="@+id/frame_25"
                android:layout_width="125dp"
                android:layout_height="56dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="22dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="136dp"
                android:clipToOutline="true"
                android:background="@drawable/frame_25"

        >
            <TextView
                    android:id="@+id/no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Отмена"
                    android:textColor="@color/black"
                    android:fontFamily="@font/opensans"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="center_horizontal|top"
                    android:textSize="22sp"
                    android:layout_centerInParent="true"/>
        </RelativeLayout>

        <RelativeLayout
                android:id="@+id/frame_26"
                android:layout_width="126dp"
                android:layout_height="56dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="174dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="136dp"
                android:clipToOutline="true"
                android:background="@drawable/frame_26"
        >
            <TextView
                    android:id="@+id/yes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Выход"
                    android:fontFamily="@font/opensans"
                    android:textSize="22sp"
                    android:textColor="@color/white"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="center_horizontal|top"
                    android:layout_centerInParent="true"/>
        </RelativeLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>