<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity_you_acc"
        >

    <View
            android:id="@+id/rectangle_5"
            android:layout_width="0dp"
            android:layout_height="194dp"
            android:layout_marginTop="-30dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/rectangle_5"
            tools:ignore="MissingConstraints"
            android:forceDarkAllowed="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"

    />

    <RelativeLayout
            android:layout_marginTop="10dp"
            android:id="@+id/navbar"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="false"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.023">

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="40dp"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/logo"
                android:id="@+id/imageView2"
                tools:ignore="MissingConstraints"/>

        <View
                android:id="@+id/profile"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="40dp"
                android:layout_centerVertical="true"
                android:background="@drawable/profile"
                tools:ignore="MissingConstraints"/>
    </RelativeLayout>


    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints">
        <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginTop="315dp"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            <TextView
                    android:fontFamily="@font/opensans"
                    android:id="@+id/some_id2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="Пол"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_weight="1"/>
            <TextView
                    android:layout_marginTop="15dp"
                    android:fontFamily="@font/opensans"
                    android:id="@+id/some_id98"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="Дата рождения"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_weight="1"/>
            <TextView
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="20dp"
                    android:fontFamily="@font/opensans"
                    android:id="@+id/some_id3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="Индивидуальная \nособенность"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout
                android:layout_marginTop="315dp"
                android:layout_marginLeft="40dp"
                android:orientation="vertical"
                android:layout_width="160dp"
                android:layout_height="wrap_content">
            <TextView
                    android:layout_marginTop="0dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginRight="0dp"
                    android:fontFamily="@font/opensans"
                    android:id="@+id/SEX"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="Мужской"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_weight="1"/>
            <TextView
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginRight="0dp"
                    android:fontFamily="@font/opensans"
                    android:id="@+id/BIRTHDAY"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="01.09.2019"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_weight="1"/>
            <TextView
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginRight="0dp"
                    android:fontFamily="@font/opensans"
                    android:id="@+id/DIAGNOS"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="ДЦП"
                    android:textAppearance="@style/some_id"
                    android:lineSpacingExtra="1sp"
                    android:translationY="-0.31sp"
                    android:gravity="top"
                    android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>


    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/frame_100">

        <ScrollView
                tools:ignore="NotSibling"
                android:layout_marginTop="450dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="55dp">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="MissingConstraints">
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                    <TextView
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/opensans"
                            android:id="@+id/some_id4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:text="Рост"
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="1sp"
                            android:translationY="-0.31sp"
                            android:gravity="top"
                            android:layout_weight="1"/>
                    <TextView
                            android:fontFamily="@font/opensans"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="1sp"
                            android:translationY="-0.31sp"
                            android:gravity="top"
                            android:layout_marginLeft="100dp"
                            android:layout_marginTop="10dp"
                            android:text="103см"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:layout_weight="1"/>
                    <View
                            android:layout_marginEnd="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/vector"
                            android:alpha="0.5"
                            android:translationY="-0.25sp"
                            tools:ignore="MissingConstraints"
                            android:id="@+id/vector_12"
                            android:layout_centerHorizontal="true"
                            app:layout_constraintHorizontal_bias="0.821"
                            app:layout_constraintVertical_bias="0.952"/>

                </LinearLayout>
                <RelativeLayout
                        android:layout_centerHorizontal="true"
                        android:layout_alignParentTop="true"
                        android:clipToOutline="true"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.494"
                        app:layout_constraintVertical_bias="0.376"
                        android:layout_width="319dp"
                        android:layout_height="138dp"
                        android:layout_marginLeft="35dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        tools:ignore="MissingConstraints"
                        android:layout_alignParentLeft="true"
                        android:gravity="center"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="35dp"
                        android:background="@drawable/cnt"
                        android:elevation="4dp"
                        android:id="@+id/frame_31"/>

                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                    <TextView
                            android:layout_marginTop="20dp"
                            android:layout_marginLeft="10dp"
                            android:fontFamily="@font/opensans"
                            android:id="@+id/some_id5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:text="Вес"
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="1sp"
                            android:translationY="-0.31sp"
                            android:gravity="top"
                            android:layout_weight="1"/>
                    <TextView
                            android:fontFamily="@font/opensans"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="1sp"
                            android:translationY="-0.31sp"
                            android:gravity="top"
                            android:layout_marginTop="20dp"
                            android:layout_marginLeft="100dp"
                            android:text="18кг"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView3"
                            android:layout_weight="1"/>
                    <View
                            android:layout_marginEnd="20dp"
                            android:layout_marginTop="20dp"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/vector"
                            android:alpha="0.5"
                            android:translationY="-0.25sp"
                            tools:ignore="MissingConstraints"
                            android:id="@+id/vector_13"
                            android:layout_centerHorizontal="true"
                            app:layout_constraintHorizontal_bias="0.821"
                            app:layout_constraintVertical_bias="0.952"/>

                </LinearLayout>

                <RelativeLayout
                        android:layout_width="319dp"
                        android:layout_height="138dp"
                        android:layout_marginLeft="35dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="20dp"
                        tools:ignore="MissingConstraints"
                        android:layout_centerHorizontal="true"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.285"
                        app:layout_constraintVertical_bias="0.0"
                        android:clipToOutline="true"
                        android:layout_alignParentLeft="true"
                        android:background="@drawable/cnt"
                        android:elevation="4dp"
                        android:id="@+id/frame_30"/>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
    <ImageView
            android:translationY="-0.25sp"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.104"
            android:id="@+id/avatar"
            android:layout_width="164dp"
            android:layout_height="164dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:background="@drawable/avatar"
    />
    <RelativeLayout
            android:id="@+id/tabbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="#FFFFFF"
            tools:ignore="MissingConstraints"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginLeft="4dp"
            android:layout_marginBottom="0dp">

        <View
                android:id="@+id/icon1"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:background="@drawable/icon__2"
                android:alpha="1"
        />

        <View
                android:id="@+id/icon2"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon1"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="0.5"/>

        <View
                android:id="@+id/icon3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon2"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon_3"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="0.5"/>

        <View
                android:id="@+id/icon4"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon3"
                android:layout_marginStart="37dp"
                android:background="@drawable/icon_4"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="0.5"/>

        <View
                android:id="@+id/icon5"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@id/icon4"
                android:layout_marginStart="35dp"
                android:layout_marginEnd="8dp"
                android:background="@drawable/icon_5"
                android:alpha="1"
                app:layout_constraintHorizontal_bias="1.0"/>

    </RelativeLayout>
    <View
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="7dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:background="@drawable/edit"
            android:alpha="0.5"
            android:translationY="-0.25sp"
            tools:ignore="MissingConstraints"
            android:id="@+id/edit"
            android:layout_centerHorizontal="true"
            app:layout_constraintHorizontal_bias="0.247"
            app:layout_constraintVertical_bias="0.348"
            app:layout_constraintStart_toEndOf="@+id/NAME"/>
    <TextView
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="-60dp"
            android:scaleType="center"
            android:translationY="-0.25sp"
            tools:ignore="MissingConstraints"
            android:layout_alignParentTop="true"
            android:lineSpacingExtra="0sp"
            android:fontFamily="@font/pattaya"
            android:layout_centerHorizontal="true"
            android:textSize="38sp"
            android:id="@+id/NAME"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Арсений"
            android:textAppearance="@style/name"
            android:gravity="top"
            app:layout_constraintHorizontal_bias="0.514"
            app:layout_constraintVertical_bias="0.338"
            app:layout_constraintTop_toBottomOf="@+id/navbar"/>


</androidx.constraintlayout.widget.ConstraintLayout>